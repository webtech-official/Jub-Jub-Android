<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jub_jub_android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".di.app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JubJubUser"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.view.notice.detail.NoticeDetailActivity" />
        <activity android:name=".ui.view.notice.NoticeActivity" />
        <activity android:name=".ui.view.main.MainActivity" />
        <activity android:name=".ui.view.rent_request.RentActivity" />
        <activity android:name=".ui.view.mypage.MyPageActivity" />
        <activity
            android:name=".ui.view.signup.SignUpActivity"
            android:windowSoftInputMode="stateAlwaysVisible" />
        <activity android:name=".ui.view.login.LoginActivity" />
        <activity
            android:name=".ui.view.splash.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>